 * @author Jeb
 */
public class Soda extends Message {
    private int NumOfSoda;
   
    public void setNumOfSoda(int newNumOfSoda){
        NumOfSoda = newNumOfSoda;
    }
    
    public int getNumOfSoda(){
        return NumOfSoda;
    }

}


